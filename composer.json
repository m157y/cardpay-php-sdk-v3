{
    "name": "m157y/cardpay-php-sdk-v3",
    "description": "PHP SDK for Unlimint API v3 with PHP8 and Guzzle7 support",
    "keywords": [
        "unlimint",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://integration.unlimint.com",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.17"
    },
    "autoload": {
        "psr-4": { "Cardpay\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Cardpay\\test\\" : "test/" }
    }
}
